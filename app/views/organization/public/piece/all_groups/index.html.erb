<%- unless @groups.empty? -%>
<ul>
  <%- @groups.each do |group| -%>
  <li>
    <%= group_replace(group, @piece.list_style) %>
    <%- unless group.public_children.empty? -%>
    <ul>
      <%- group.public_children.each do |child| -%>
      <%= group_li(child, @piece.list_style) %>
      <%- end -%>
    </ul>
    <%- end -%>
  </li>
  <%- end -%>
</ul>
<%- end -%>
